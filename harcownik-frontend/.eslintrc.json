{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-eval": "error",
        "semi": ["error", "always"],
        "eqeqeq": "error",
        "prefer-arrow-callback": "error",
        "no-var": "error",
        "strict": ["error", "global"],
        "prefer-template": "error",
        "quotes": ["error", "double"]
    }
}
